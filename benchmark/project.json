        "protobuf-net": "2.1.0",
        "Google.Protobuf": "3.1.0",
        "Bond.Compiler": "5.1.0",
        "Bond.CSharp": "5.1.0",

        "serializers": "1.0.0",
        "utils": "1.0.0"
      },
      "imports": [
        "dnxcore50",
        "portable-net45+win8",
        "netstandard1.6"
