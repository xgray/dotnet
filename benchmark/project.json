  "dependencies": {},
  "frameworks": {
    "netcoreapp1.1": {
      "buildOptions": {
        "define": [ "CORE", "SILVERLIGHT" ]
      },
      "dependencies": {

        "Microsoft.NETCore.App": { 
          "type": "platform", 
          "version": "1.1.0"
        },

        "System.Collections": "4.3.0",
        "System.Collections.Concurrent": "4.3.0",
        "System.ComponentModel": "4.3.0",
